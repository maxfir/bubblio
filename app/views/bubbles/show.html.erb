<%#KILL ALL EXPIRED LINKS %>
<% cleanLinks(@bubble) %>

<% provide(:title, @bubble.title) %>
<p>This is bubble of title <%= @bubble.title%></p>
<%= link_to "add something", new_bubble_item_path(@bubble) %>
<%#= render 'items/form' %>
<% if @bubble.items.any? %>
	<%= render @bubble.items %>
<% else %>
	<p>No files uploaded yet</p>
<% end %>
<%= link_to "generate links", new_bubble_link_path(@bubble) %>
<%#= render :partial => 'items/form' %>
<%= render @bubble.links %>